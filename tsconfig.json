{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "lib": ["dom", "es2017"],
      "jsx": "react",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src"]
  }
  